if tenantid == "p1":
    with open(mapping_file, "r", encoding="utf-8") as file:
        for line in file:
            parts = line.split(":")
            if len(parts) < 2:
                continue  # Skip malformed lines

            left_side = parts[0].strip()
            if f"_{search_key}_" in left_side:
                link = parts[1].strip()
                
                match = re.search(r'_(\d+)\.', link)
                if match:
                    drlocationsa = match.group(1)